{
    "name": "leroy/flysystem-skynet",
    "description": "Flysystem Adapter for Skynet",
    "type": "library",
    "require": {
        "php": "^7.4",
        "league/flysystem": "^1.0",
        "graham-campbell/guzzle-factory": "^3.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LeroyShirto\\FlysystemSkynet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeroyShirto\\FlysystemSkynet\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
